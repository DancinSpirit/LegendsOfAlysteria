<div id = "turn-title">
    <p class="boxtext big-boy big-boy-turn-title"></p>
    <div id="title-box-turn-title">
        <p class="boxtext turn-title">Turn <%=turn%></p>
        <p class="boxtext turn-subtitle">
            <%if(season=="0"){%>
                Winter
            <%}%>
            <%if(season=="1"){%>
                Spring
            <%}%>
            <%if(season=="2"){%>
                Summer
            <%}%>
            <%if(season=="3"){%>
                Fall
            <%}%>
            of Year <%=year%></p>
    </div>
    <p class="boxtext big-boy big-boy-turn-title"></p>
</div> 
<div class = "height-box">

</div>
<background>https://media.discordapp.net/attachments/948762262829666325/948765394330595388/Book.jpg</background>
<script>
    $("#left-arrow-box").removeClass("invisible");
    $("#top-arrow-box").removeClass("invisible");
    $("#right-arrow-box").removeClass("invisible");
    $("#gamemaster-tab").addClass("unselected")
    $("#gamemaster-tab").removeClass("invisible");
    $("#contents-tab").addClass("unselected");
    $("#contents-tab").removeClass("invisible");
    $("#story-tab").removeClass("unselected")
    $("#left-arrow-box").off("click");
    $("#left-arrow-box").on("click",function(){
            if(customData[1].season==0){
                if(customData[1].year==0){
                    if(databaseObjects[0][0].id == "613914ffd4d10a12926304cd"){
                        states = ["story","disclaimer"]
                        databaseObjects = databaseObjects;
                        customData = customData;
                        window.history.pushState({states:states,databaseObjects: databaseObjects,customData:customData}, "Story Disclaimer", "/story/disclaimer");
                        loadState(1,"right");  
                    }else{
                        states = ["story","main-title"]
                        databaseObjects = databaseObjects;
                        customData = customData;
                        window.history.pushState({states:states,databaseObjects: databaseObjects,customData:customData}, "Story Main Title", "/story/main-title");
                        loadState(1,"right");
                    }
                }else{
                    customData[1].year--;
                    customData[1].season=3;
                    customData[1].regionPhase=story.years[customData[1].year].seasons[customData[1].season].regionPhases.length-1;
                    customData[1].rulerPhase=story.years[customData[1].year].seasons[customData[1].season].regionPhases[customData[1].regionPhase].rulerPhases.length-1;
                    customData[1].eventIndex=story.years[customData[1].year].seasons[customData[1].season].regionPhases[customData[1].regionPhase].rulerPhases[customData[1].rulerPhase].events.length-1;
                    customData[1].turn--;
                    states = ["story","event"];
                    databaseObjects[1].push({name:"Event",id:story.years[customData[1].year].seasons[customData[1].season].regionPhases[customData[1].regionPhase].rulerPhases[customData[1].rulerPhase].events[customData[1].eventIndex]})
                    window.history.pushState({states:states,databaseObjects: databaseObjects,customData:customData}, "Story Event", "/story/event");
                    loadState(1,"right");
                }
            }else{
                customData[1].season--;
                customData[1].regionPhase=story.years[customData[1].year].seasons[customData[1].season].regionPhases.length-1;
                customData[1].rulerPhase=story.years[customData[1].year].seasons[customData[1].season].regionPhases[customData[1].regionPhase].rulerPhases.length-1;
                customData[1].eventIndex=story.years[customData[1].year].seasons[customData[1].season].regionPhases[customData[1].regionPhase].rulerPhases[customData[1].rulerPhase].events.length-1;
                customData[1].turn--;
                states = ["story","event"];
                databaseObjects[1].push({name:"Event",id:story.years[customData[1].year].seasons[customData[1].season].regionPhases[customData[1].regionPhase].rulerPhases[customData[1].rulerPhase].events[customData[1].eventIndex]})
                window.history.pushState({states:states,databaseObjects: databaseObjects,customData:customData}, "Story Event", "/story/event");
                loadState(1,"right");
            }
    })
    $("#right-arrow-box").off("click");
    $("#right-arrow-box").on("click", function(){
        states = ["story","region-title"]
        databaseObjects = databaseObjects;
        customData[1].regionPhase = 0;
        customData = [customData[0],customData[1]];
        window.history.pushState({states:states,databaseObjects: databaseObjects,customData:customData}, "Story Region Title", "/story/region-title");
        loadState(1,"left");
    })
    activateTitleClick();
</script>