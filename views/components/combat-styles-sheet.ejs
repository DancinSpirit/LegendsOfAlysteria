                                                <%}else if(combatStyles[x].rerolls[y].amount>0){%>
                                                    <%=combatStyles[x].fightingStyles[0].reroll.name%>
                                                <%}else{%>
                                                    None
                                                <%}%>
                                            <%}%>
                                        <%}%> 
                                    <%}else{%>
                                        None
                                    <%}%>                          
